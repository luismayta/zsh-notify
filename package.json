{
  "keywords": [],
  "name": "@luismayta/zsh-notify",
  "description": "zsh-notify for zsh",
  "repository": "ssh://git@github.com/luismayta/zsh-notify.git",
  "author": "Luis Mayta <slovacus@gmail.com>",
  "license": "MIT",
  "stylintrc": ".stylintrc",
  "version": "0.1.0",
  "scripts": {
    "clean": "rimraf dist .next out build",
    "clean:all": "yarn clean node_modules",
    "lint": "eslint -c ./.eslintrc.js ./ --ext .js,.jsx,.mjs,.d.ts,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "lint:less": "stylelint --config=.stylintrc --syntax less **/*.less",
    "lint:less:fix": "yarn lint:less --fix",
    "prettier:lint": "prettier '**/*.{js?(on),ts?(x),scss,md,y?(a)ml,gql,graphql?(s),less,mjml}' --write=false --list-different",
    "prettier:fix": "yarn prettier:lint --write=true",
    "precommit": "lint-staged"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@equipindustry/eslint-config": "^0.3.4",
    "@equipindustry/prettierrc": "^0.2.0",
    "@types/jest": "^26.0.13",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "axios": "^0.20.0",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "==6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "node-plop": "^0.26.2",
    "plop": "^2.6.0",
    "prettier": "^2.1.1",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.1",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "mjml": "^4.6.3",
    "sendmail": "^1.6.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
